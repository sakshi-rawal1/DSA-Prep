import java.util.*;
import java.lang.*;
import java.io.*;

class Ideone
{
	public static void main (String[] args) throws java.lang.Exception
	{
	//Reverse an Array	
	int[] arr = {2,4,5,1,3,8};
	int i = 0;
	int j = arr.length -1;
	while(i<j){
		int temp = arr[i];
		arr[i]=arr[j];
		arr[j]=temp;
		j--;
		i++;
		
	}
	System.out.println(Arrays.toString(arr));
	}
}
